@page 
@model PDFMenu.Pages.Features.Admins.HistoryModel
@{

}
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" />
<link rel="stylesheet" href="~/css/Admins/History.css" />
<header>
      <div class="header-left">
        <img src="~/Images/logo.png" alt="Logo" width="100" height="60" class="float-start header-logo">
        <div class="DineDocs">
          <p>DineDocs</p>
        </div>
      </div>
  
      <div class="header-right">
        <a href="/Index" class="header-button">Home</a>
        <a href="/Features/NormalUsers/ContactUs" class="header-button">Contact Us</a>
        <a href="/Features/NormalUsers/About" class="header-button">About</a>
      </div>
  </header>

   <div class="container">
       <h1>History</h1>
        <div class="container1" >


            
        <table>
            <tr>
                <th>Menu Name</th>
                <th>Link</th>
                <th>Date</th>
       
            </tr>

   
          @foreach (var history in @Model.MenuUploads)
    {
        <tr>
            <td>@history.menu_pdf</td>
            <td><a href="/Features/NormalUsers/PDFMenu/@history.menu_pdf">View PDF Menu</a></td>
            <td>@history.menu_upload_date</td>
        </tr>
    }

        </table>

        </div>
   </div>
